function qb --description 'Launch qutebrowser web browser'
    # Check if qutebrowser is installed
    if not command -v qutebrowser &>/dev/null
        echo "qutebrowser is not installed. Run 'chezmoi apply' to install."
        return 1
    end

    # Launch qutebrowser (detached from terminal)
    qutebrowser $argv &>/dev/null &
    disown
end