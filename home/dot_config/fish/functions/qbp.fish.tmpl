function qbp --description 'Launch qutebrowser in private browsing mode'
    # Check if qutebrowser is installed
    if not command -v qutebrowser &>/dev/null
        echo "qutebrowser is not installed. Run 'chezmoi apply' to install."
        return 1
    end

    # Launch qutebrowser in private mode (detached from terminal)
    qutebrowser --target private-window $argv &>/dev/null &
    disown
end